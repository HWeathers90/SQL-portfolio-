SELECT product,
SUM (quantity*price)
FROM BIT_DB.JanSales
GROUP BY product
ORDER BY sum(quantity*price) desc
LIMIT 1;

Who are the top 10 artist based on popularity?
SELECT popularity, artist_name
FROM BIT_DB.Spotifydata
GROUP BY artist_name
ORDER BY popularity LIMIT 10;

SELECT MAX(tempo), artist_name
FROM BIT_DB.Spotifydata;

SELECT MAX(duration_ms), artist_name
FROM BIT_DB.Spotifydata;

SELECT AVG(danceability),artist_name, popularity
FROM BIT_DB.Spotifydata
GROUP BY artist_name
ORDER BY AVG(danceability) desc LIMIT 10;
